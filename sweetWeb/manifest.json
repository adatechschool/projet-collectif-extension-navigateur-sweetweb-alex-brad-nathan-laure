{
  "manifest_version": 3,
  "name": "Message Test",
  "version": "1.0",
  "description": "Practice Test",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "activeTab",
    "tabs",
    "scripting"
  ]
}